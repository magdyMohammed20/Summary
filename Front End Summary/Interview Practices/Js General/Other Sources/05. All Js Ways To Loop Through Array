
// For Loop
var forArr = [1,2,3,4]

for(var x = 0; x<forArr.length; x++){
    console.log(x , forArr[x])
}


// While Loop
var whileArr = [1,2,3,4]

var x = 0

while(x < whileArr.length){
    console.log(x , whileArr[x])
    x++
}


// forEach Method ES5
var forEachArr = [1,2,3,4]

forEachArr.forEach((element , index , array) => {
    console.log(index , element , array)
})


// map Method
var mapArr = [1,2,3,4]

mapArr.map((element , index , array) => {
    console.log(element , index , array)
})


// Filter Method
var filterArr = [1,2,3,4]

var newArray = filterArr.filter((element , index , array) => {
    return element > 2
})

console.log(newArray)


// Reduce Method
var reduceArr = [1,2,3,4]

var newArray = reduceArr.reduce((acc , ele , index , arr)=>{
    return acc += ele
} , 0) // 0 Here Is Initial Value For acc And Is Optional And Default Value For It Is 0


console.log(newArray)


// For...of Loop
var forOfArr = [1,2,3,4]
for(value of forOfArr){
    console.log(value)
}

